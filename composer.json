{
  "name": "cmdrsharp/guzzle-api",
  "description": "A cleaner approach to using Guzzle in Laravel",
  "type": "libr",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": ">=6.3",
    "illuminate/support": ">=5.8"
  },
  "require-dev": {
    "phpunit/phpunit": "8.0.*",
    "dms/phpunit-arraysubset-asserts": ">=0.1.0"
  },
  "prefer-stable": true,
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "CmdrSharp\\GuzzleApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CmdrSharp\\GuzzleApi\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CmdrSharp\\GuzzleApi\\GuzzleApiServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
