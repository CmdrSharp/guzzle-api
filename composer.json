{
  "name": "cmdrsharp/guzzle-api",
  "description": "A cleaner approach to using Guzzle in Laravel",
  "type": "libr",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "5.5.*||5.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "prefer-stable": true,
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Cmdrsharp\\GuzzleApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cmdrsharp\\GuzzleApi\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cmdrsharp\\GuzzleApi\\GuzzleApiServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
